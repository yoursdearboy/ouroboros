{
    "select": {
        "columns": [
            "patients.id",
            "patients.last_name",
            "donorships.date",
            "donors.last_name"
        ],
        "from": [
            {
                "left": {
                    "left": "patients",
                    "right": "donorships",
                    "on": "donorships.patient_id = patients.id"
                },
                "right": "donors",
                "on": "donors.id = donorships.donor_id"
            }
        ]
    }
}
